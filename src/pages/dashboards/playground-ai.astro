---
export const prerender = false;
import PageLayout from "@/layouts/PageLayout.astro";
import PlaygroundAiAppComponent from "@/components/portfolio/dashboards/playground-ai";
import ContainerSection from "@/components/common/ContainerSection.astro";
import HeaderPortfolio from "@/components/portfolio/common/header-portfolio";
import { pagesSeo } from "@/config/pages";

const meta = pagesSeo["playground-ai"];
---

<PageLayout meta={meta}>
  <ContainerSection>
    <HeaderPortfolio
      client:visible
      title={"Playground AI"}
      description={"A user-friendly interface integrating OpenAI's ChatGPT, allowing interactive AI model settings and responses."}
      chipsList={[
        "React",
        "Typescript",
        "MobX",
        "Tailwind CSS",
        "Shadcn UI",
        "Radix UI",
        "OpenAI API",
      ]}
    />
    <PlaygroundAiAppComponent client:only />
  </ContainerSection>
</PageLayout>
