---
import PageLayout from "@/layouts/PageLayout.astro";
import ContainerSection from "@/components/common/ContainerSection.astro";
import WebcamDetectionComponent from "@/components/portfolio/webcam-detection";
import HeaderPortfolio from "@/components/portfolio/common/header-portfolio";
import type mod from "astro/zod";
---

<PageLayout meta={{ title: "Webcam Detection App" }}>
	<ContainerSection maxWidth="max-w-2xl">
		<HeaderPortfolio
			client:visible
			title={"Webcam Detection"}
			description={"Image object detection in webcam"}
			chipsList={["React", "Typescript", "react-webcam", "Coco-ssd", "Tensorflow.js"]}
		/>
		<WebcamDetectionComponent client:visible />
	</ContainerSection>
</PageLayout>
